e {\n                setData({ ...data, categories: result })\n            }\n        })\n    }\n    useEffect(() => {\n        loadCategory()\n    }, [])\n\n    const viewCategorys = () => (\n        categories.data && categories.data.map((category, i) => (\n\n            <option key={i}  value={category._id}>{category.name}</option>\n\n        ))\n    )\n    const handleChange = name=>e => {\n   \n        setData({ ...data, [name]:e.target.value ,searched:false })\n    }\n   const searchData = () =>{\n       if(search){\n\n           listBySearch({search:search || undefined ,category:category}).then(result=>{\n               if(result.error){\n                setError(result.error)\n\n\n               }else{\n                   setData({...data , results:result , searched:true})\n\n               }\n           })\n       }\n   }\n    const submitSearch = (e) => {\n        e.preventDefault()\n        searchData()\n\n    }\n\n    const searchForm = () => (\n        <form onSubmit={submitSearch} className=\"mb-4\">\n            <span className=\"input-group-text\">\n                <div className=\"input-group input-group-lg\">\n                    <div className=\"input-group-prepend\">\n                        <select onChange={handleChange('category')} className=\"btn mr-2\">\n                            <option value=\"All\">Select Category</option>\n                            {viewCategorys()}\n                        </select>\n                    </div>\n\n\n                    <input type=\"text\" onChange={handleChange(\"search\")} className=\"form-control\" placeholder=\"Search By Name ..\" />\n                </div>\n                <div className=\"btn input-group-append\" style={{border : 'none' , cursor:'pointer'}}>\n                        <button className=\"input-group-text\">Search</button>\n                </div>\n            </span>\n\n\n        </form>\n    )\n\nconst searchMessage = (searched ,results) =>{\n    if(searched && results.length > 0){\n        return `Found ${results.length} products`\n    }\n    if(searched && results.length < 1){\n        return `No Products Found`\n    }\n}\nconst viewProduct = (results=[])=>{\n\n    return (\n        <Fragment>\n            <h4 className=\" text-center mb-3 mt-3\">\n                {searchMessage(searched ,results)}\n\n            </h4>\n            <br/>\n            <div className=\"row pb-5 pt-4 mb-4\">\n        {results && results.map((product, i)=>(\n            <ProductCard key={i} product={product} />\n            ))}\n            </div>\n            </Fragment>\n            )\n    }\n    \n\n\n    return (\n        <div  >\n            {searchForm()}\n\n            \n\n              {viewProduct(results)}\n          \n\n        </div>\n    )\n}\n\n\nexport default Search","/Users/amiar/Downloads/Ecommerce-Application-in-react-node-main/clients/src/shop/productImage.js",[],{"ruleId":"131","replacedBy":"132"},{"ruleId":"133","replacedBy":"134"},{"ruleId":"135","severity":1,"message":"136","line":29,"column":9,"nodeType":"137","messageId":"138","endLine":29,"endColumn":17},{"ruleId":"135","severity":1,"message":"139","line":30,"column":9,"nodeType":"137","messageId":"138","endLine":30,"endColumn":17},{"ruleId":"135","severity":1,"message":"140","line":36,"column":9,"nodeType":"137","messageId":"138","endLine":36,"endColumn":26},{"ruleId":"141","severity":1,"message":"142","line":63,"column":8,"nodeType":"143","endLine":63,"endColumn":10,"suggestions":"144"},{"ruleId":"135","severity":1,"message":"145","line":1,"column":17,"nodeType":"137","messageId":"138","endLine":1,"endColumn":26},{"ruleId":"135","severity":1,"message":"146","line":14,"column":12,"nodeType":"137","messageId":"138","endLine":14,"endColumn":17},{"ruleId":"135","severity":1,"message":"147","line":15,"column":19,"nodeType":"137","messageId":"138","endLine":15,"endColumn":27},{"ruleId":"141","severity":1,"message":"148","line":85,"column":8,"nodeType":"143","endLine":85,"endColumn":10,"