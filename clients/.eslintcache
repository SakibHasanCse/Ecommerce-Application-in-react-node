sole.log(res)\r\n    //             return res.json()\r\n    //         })\r\n    //         .catch(err => {\r\n    //             console.log(err)\r\n    //         })\r\n\r\n    // }\r\n    const fromSubmit = (event) => {\r\n        event.preventDefault();\r\n        setValues({ ...values, error: false })\r\n        signup({ name, email, password })\r\n            .then((res) => {\r\n                if (res.error) {\r\n                    setValues({ ...values, error: res.error, success: false })\r\n                } else {\r\n                    setValues({\r\n                        ...values,\r\n                        name: \"\",\r\n                        email: \"\",\r\n                        password: \"\",\r\n                        success: true,\r\n                        error: ''\r\n                    })\r\n\r\n                }\r\n            })\r\n    }\r\n\r\n\r\n    const signupForm = () => (\r\n        <form >\r\n            <div className=\"form-group\">\r\n                <label className=\"text-muted\">Name</label>\r\n                <input type=\"text\" onChange={handleChange(\"name\")} value={name} className=\"form-control\" placeholder=\"\" />\r\n            </div>\r\n            <div className=\"form-group\">\r\n                <label className=\"text-muted\">Email</label>\r\n                <input type=\"email\" onChange={handleChange(\"email\")} value={email} className=\"form-control\" name=\"email\" placeholder=\"\" />\r\n            </div><div className=\"form-group\">\r\n                <label className=\"text-muted\">Password</label>\r\n                <input type=\"password\" onChange={handleChange(\"password\")} value={password} className=\"form-control\" name=\"password\" placeholder=\"\" />\r\n            </div>\r\n            <button onClick={fromSubmit} className=\"btn btn-primary\">Submit</button>\r\n        </form>\r\n    )\r\n\r\n    const errorMessage = () =>\r\n    (<div className=\"alert alert-danger\" role=\"alert\" style={{ display: error ? '' : 'none' }} >\r\n        {error}\r\n    </div>)\r\n\r\n    const successMessage = () =>\r\n    (<div className=\"alert alert-success\" role=\"alert\" style={{ display: success ? '' : 'none' }}>\r\n        Account Created Successfully\r\n    </div>)\r\n\r\n    return (\r\n\r\n        <Layout title=\"Signup Page\" description=\"This is the Signup page\" className=\"container col-md-8 offset-md-2\">\r\n            {errorMessage()}\r\n            {successMessage()}\r\n            {signupForm()}\r\n\r\n        </Layout>\r\n\r\n    );\r\n}\r\n\r\nexport default Signup;\r\n","H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\user\\singin.js",[],"H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\shop\\manu.js",[],"H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\shop\\home.js",["101"],"H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\config.js",[],"H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\shop\\layout.js",[],"H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\auth\\index.js",[],"H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\user\\userDashbord.js",[],"H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\auth\\privateRoute.js",["102"],"import React, { Component } from 'react';\r\nimport { Redirect, Route } from 'react-router-dom';\r\nimport { isAuthnticated } from './index';\r\n\r\nfunction PrivateRoute({ component: Component, ...rest }) {\r\n    return (\r\n\r\n        <Route\r\n            {...rest}\r\n            render={props =>\r\n                isAuthnticated() ? (\r\n                    <Component />\r\n                ) : (<Redirect\r\n                    to={{\r\n                        pathname: \"/signin\",\r\n                        state: { from: props.location }\r\n                    }}\r\n                />\r\n                    )}\r\n        />\r\n    )\r\n\r\n\r\n}\r\nexport default PrivateRoute","H:\\bako\\pr