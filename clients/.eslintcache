ssage = () =>\r\n    (<div className=\"alert alert-danger\" role=\"alert\" style={{ display: error ? '' : 'none' }} >\r\n        {error}\r\n    </div>)\r\n\r\n    const successMessage = () =>\r\n    (<div className=\"alert alert-success\" role=\"alert\" style={{ display: success ? '' : 'none' }}>\r\n        Account Created Successfully\r\n    </div>)\r\n\r\n    return (\r\n\r\n        <Layout title=\"Signup Page\" description=\"This is the Signup page\" className=\"container col-md-8 offset-md-2\">\r\n            {errorMessage()}\r\n            {successMessage()}\r\n            {signupForm()}\r\n\r\n        </Layout>\r\n\r\n    );\r\n}\r\n\r\nexport default Signup;\r\n","H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\user\\singin.js",[],"H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\shop\\manu.js",[],"H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\shop\\home.js",[],"H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\config.js",[],"H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\shop\\layout.js",[],["75","76"],"H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\auth\\index.js",[],"H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\user\\userDashbord.js",[],"H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\auth\\privateRoute.js",["77"],"import React, { Component } from 'react';\r\nimport { Redirect, Route } from 'react-router-dom';\r\nimport { isAuthnticated } from './index';\r\n\r\nfunction PrivateRoute({ component: Component, ...rest }) {\r\n    return (\r\n\r\n        <Route\r\n            {...rest}\r\n            render={props =>\r\n                isAuthnticated() ? (\r\n                    <Component />\r\n                ) : (<Redirect\r\n                    to={{\r\n                        pathname: \"/signin\",\r\n                        state: { from: props.location }\r\n                    }}\r\n                />\r\n                    