ched && results.length < 1){\r\n        return `No Products Found`\r\n    }\r\n}\r\nconst viewProduct = (results=[])=>{\r\n\r\n    return (\r\n        <Fragment>\r\n            <h4 className=\" text-center mb-3 mt-3\">\r\n                {searchMessage(searched ,results)}\r\n\r\n            </h4>\r\n            <br/>\r\n            <div className=\"row pb-5 pt-4 mb-4\">\r\n        {results && results.map((product, i)=>(\r\n            <ProductCard key={i} product={product} />\r\n            ))}\r\n            </div>\r\n            </Fragment>\r\n            )\r\n    }\r\n    \r\n\r\n\r\n    return (\r\n        <div  >\r\n            {searchForm()}\r\n\r\n            \r\n\r\n              {viewProduct(results)}\r\n          \r\n\r\n        </div>\r\n    )\r\n}\r\n\r\n\r\nexport default Search","H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\shop\\singleProduct.js",["129"],{"ruleId":"130","replacedBy":"131"},{"ruleId":"132","replacedBy":"133"},{"ruleId":"134","severity":1,"message":"135","line":2,"column":10,"nodeType":"136","messageId":"137","endLine":2,"endColumn":13},{"ruleId":"134","severity":1,"message":"138","line":10,"column":12,"nodeType":"136","messageId":"137","endLine":10,"endColumn":17},{"ruleId":"134","severity":1,"message":"139","line":1,"column":17,"nodeType":"136","messageId":"137","endLine":1,"endColumn":26},{"ruleId":"134","severity":1,"message":"139","line":1,"column":17,"nodeType":"136","messageId":"137","endLine":1,"endColumn":26},{"ruleId":"134","severity":1,"message":"140","line":29,"column":9,"nodeType":"136","messageId":"137","endLine":29,"endColumn":17},{"ruleId":"134","severity":1,"message":"141","line":30,"column":9,"nodeType":"136","messageId":"137","endLine":30,"endColumn":17},{"ruleId":"134","severity":1,"message":"142","line":36,"column":9,"nodeType":"136","messageId":"137","endLine":36,"endColumn":26},{"ruleId":"143","severity":1,"message":"144","line":63,"column":8,"nodeType":"145","endLine":63,"endColumn":10,"suggest