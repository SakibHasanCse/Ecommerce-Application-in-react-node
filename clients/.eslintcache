n }\r\n                    }}\r\n                />\r\n                    )}\r\n        />\r\n    )\r\n\r\n\r\n}\r\nexport default PrivateRoute","H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\auth\\adminPrivate.js",["106"],"import React, { Component } from 'react';\r\nimport { Redirect, Route } from 'react-router-dom';\r\nimport { isAuthnticated } from './index';\r\n\r\nfunction adminRouter({ component: Component, ...rest }) {\r\n    return (\r\n\r\n        <Route\r\n            {...rest}\r\n            render={props =>\r\n                isAuthnticated() && isAuthnticated().user.role === 'admin' ? (\r\n                    <Component />\r\n                ) : (<Redirect\r\n                    to={{\r\n                        pathname: \"/signin\",\r\n                        state: { from: props.location }\r\n                    }}\r\n                />\r\n                    )}\r\n        />\r\n    )\r\n\r\n\r\n}\r\nexport default adminRouter","H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\user\\adminDashbord.js",[],"H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\admin\\adminApi.js",[],"H:\\bako\\preactis\\react and nodejs Ecommer Pre\\clients\\src\\admin\\AddProduct.js",["107","108","109","110"],"\r\nimport React, { useEffect, useState } from 'react';\r\nimport { isAuthnticated } from '../auth/index';\r\nimport Layout from '../shop/layout';\r\nimport { AllCategoryAPi, ProdductAPi } from './adminApi';\r\n\r\nconst CreateProduct = () => {\r\n\r\n    const [values, setValues] = useState({\r\n        name: \"\",\r\n        description: \"\",\r\n        photo: \"\",\r\n        price: \"\",\r\n        category: \"\",\r\n        shipping: \"\",\r\n        categories: [],\r\n        quntity: \"\",\r\n        loading: false,\r\n        error: '',\r\n        createdProduct: '',\r\n        redirectToProfile: false,\r\n        formData: ''\r\n\r\n    })\r\n\r\n    const { name,\r\n        description,\r\n        price,\r\n 